{
  "name": "whatsapp-assistant-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "node ../node_modules/tsx/dist/cli.mjs watch src/index.ts",
    "start": "node dist/index.js",
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up --dir migrations/run",
    "migrate:down": "node-pg-migrate down --dir migrations/run"
  },
  "dependencies": {
    "bull": "^4.16.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "groq-sdk": "^0.5.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "@huggingface/transformers": "^3.4.0",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.13.1",
    "pgvector": "^0.2.0",
    "qrcode": "^1.5.4",
    "uuid": "^10.0.0",
    "whatsapp-web.js": "^1.26.0",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
